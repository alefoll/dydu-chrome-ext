{
  "name": "Do You Dream Up DevTools",
  "version": "0.1.0",
  "description": "Extends the Developer Tools, adding tools for Do You Dream Up chatbox's.",
  "devtools_page": "devtools.html",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "<all_urls>"
  ],
    "background": {
    "scripts": ["background.js"]
  },
  "minimum_chrome_version": "50.0"
}
